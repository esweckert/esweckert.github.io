<mvc:View height="100%"
	controllerName="com.esweckert.profile.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt">
	<tnt:ToolPage
		id="app"
		>
		<tnt:header>
			<tnt:ToolHeader>
				<Image src="images/wecker_logo_1.jpg" decorative="false" tooltip="T.F. Wecker Logo" press="onToHome">

			</Image>

				<Title
					text="{i18n>appTitle}"
					level="H2"/>
			
				<ToolbarSpacer/>

			</tnt:ToolHeader>
		</tnt:header>

		<tnt:mainContents>
			<App id="mainContents"/>
		</tnt:mainContents>


	</tnt:ToolPage>

	
</mvc:View>
